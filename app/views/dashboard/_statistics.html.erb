<%= javascript_tag do %>
  $(document).ready(function() {

    $('#statistics-nav-tab a[data-toggle="tab"]').on('show', function (e) {
      var target = e.target.getAttribute("href");
      var target_path = '<%= @presenter.graph_prefix_uri %>' + target.substring(1);
      $(target).load(target_path);
    });

    $('#statistics-nav-tab a').click(function (e) {
      e.preventDefault();
      $(this).tab('show');
    })

    $('#statistics-nav-tab a:first').tab('show');
  });
<% end %>

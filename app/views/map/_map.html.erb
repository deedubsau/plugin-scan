 <script type='text/javascript' src='https://www.google.com/jsapi'></script>
    <script type='text/javascript'>
     google.load('visualization', '1', {'packages': ['geochart']});
     google.setOnLoadCallback(drawRegionsMap);

      function drawRegionsMap() {
        var map_data = google.visualization.arrayToDataTable([
          ['Country', 'Exploitable'],
          <%= @map_data %>
        ]);

        var map_options = {};

        var map_chart = new google.visualization.GeoChart(document.getElementById('map_div'));

        map_chart.draw(map_data, map_options);
    };
    </script>
    <div id="map_div" style="width: 900px; height: 500px;"></div>
